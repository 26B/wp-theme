  "browser": {},
  "browserify": {
    "transform": [
      "envify",
      "babelify",
      "browserify-shim",
      "aliasify",
      "browserify-data"
    ]
  },
  "aliasify": {
    "aliases": {
      "underscore": "lodash"
    }
  },
  "browserify-shim": {
    "jquery": "global:jQuery"
  },
  "scripts": {
    "build": "gulp build"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "plugins": [
      "babel",
      "prettier"
    ],
    "env": {
      "browser": true,
      "node": false
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true,
        "modules": true
      }
    }
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "aliasify": "^2.0.0",
    "autoprefixer": "^6.3.6",
    "babel-eslint": "^8.0.2",
    "babelify": "^7.3.0",
    "browser-cookies": "^1.1.0",
    "browser-locale": "^1.0.0",
    "browser-sync": "^2.26.3",
    "browserify": "^13.0.1",
    "browserify-data": "^0.2.0",
    "browserify-hmr": "^0.3.1",
    "browserify-shim": "~3.8.12",
    "cssnano": "^3.6.2",
    "del": "^2.2.0",
    "envify": "^3.4.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^2.8.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.3.1",
    "glob": "^7.0.3",
    "gulp": "^4.0.0",
    "gulp-cached": "^1.1.0",
    "gulp-changed": "^1.3.0",
    "gulp-concat": "^2.6.0",
    "gulp-eslint": "^4.0.0",
    "gulp-filter": "^4.0.0",
    "gulp-font2css": "^0.1.2",
    "gulp-if": "^2.0.1",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^6.1.1",
    "gulp-sass": "^3.1.0",
    "gulp-sort": "^2.0.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-stylelint": "^5.0.0",
    "gulp-svg-sprite": "^1.3.6",
    "gulp-util": "^3.0.7",
    "gulp-wp-pot": "^1.3.2",
    "has-flag": "^2.0.0",
    "lodash": "^4.17.4",
    "modularized-normalize-scss": "^4.0.0",
    "pixrem": "^3.0.1",
